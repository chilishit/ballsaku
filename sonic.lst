00001462 E34A                       		lsl.w	#1,d2
00001464 6400                       		bcc.s	loc_146A	; branch if the sign bit isn't set
00001466 D6FC 0000                  		adda.w	#NemPCD_WriteRowToVDP_XOR-NemPCD_WriteRowToVDP,a3	; otherwise the file uses XOR mode
