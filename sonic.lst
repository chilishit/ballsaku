0000146C 3A42                       		movea.w	d2,a5	; and store it in a5 because there aren't any spare data registers
0000146E 7608                       		moveq	#8,d3	; 8 pixels in a pattern row
00001470 7400                       		moveq	#0,d2
00001472 7800                       		moveq	#0,d4
00001474 6100 0000                  		bsr.w	NemDec_BuildCodeTable
00001478 1A18                       		move.b	(a0)+,d5	; get first byte of compressed data
0000147A E145                       		asl.w	#8,d5	; shift up by a byte
0000147C 1A18                       		move.b	(a0)+,d5	; get second byte of compressed data
0000147E 3C3C 0010                  		move.w	#$10,d6	; set initial shift value
00001482 6100                       		bsr.s	NemDec_ProcessCompressedData
00001484 4CDF 3BFF                  		movem.l	(sp)+,d0-a1/a3-a5
