00001578 5440                       		addq.w	#2,d0	; increment index
0000157A 51CD FFF8                  		dbf	d5,NemBCT_ShortCode_Loop	; repeat for required number of entries
0000157E 60BA                       		bra.s	NemBCT_Loop
