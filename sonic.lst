0000153A                            
0000153A                            NemBCT_Loop:
0000153A 1018                       		move.b	(a0)+,d0	; read next byte
0000153C 0C00 0080                  		cmpi.b	#$80,d0	; sign bit being set signifies a new palette index
00001540 64EE                       		bcc.s	NemBCT_ChkEnd	; a bmi could have been used instead of a compare and bcc
00001542                            		
00001542 1200                       		move.b	d0,d1
00001544 0247 000F                  		andi.w	#$F,d7	; get palette index
00001548 0241 0070                  		andi.w	#$70,d1	; get repeat count for palette index
0000154C 8E41                       		or.w	d1,d7	; combine the two
0000154E 0240 000F                  		andi.w	#$F,d0	; get the length of the code in bits
00001552 1200                       		move.b	d0,d1
00001554 E149                       		lsl.w	#8,d1
00001556 8E41                       		or.w	d1,d7	; combine with palette index and repeat count to form code table entry
00001558 7208                       		moveq	#8,d1
0000155A 9240                       		sub.w	d0,d1	; is the code 8 bits long?
0000155C 6600                       		bne.s	NemBCT_ShortCode	; if not, a bit of extra processing is needed
0000155E 1018                       		move.b	(a0)+,d0	; get code
00001560 D040                       		add.w	d0,d0	; each code gets a word-sized entry in the table
00001562 3387 0000                  		move.w	d7,(a1,d0.w)	; store the entry for the code
