0000156C D040                       		add.w	d0,d0	; shift so that high bit is in bit position 7
0000156E 7A01                       		moveq	#1,d5
00001570 E36D                       		lsl.w	d1,d5
00001572 5345                       		subq.w	#1,d5	; d5 = 2^d1 - 1
